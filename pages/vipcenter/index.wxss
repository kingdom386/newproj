/* pages/vipcenter/index.wxss */
.center_box image{
  width: 750rpx;
  height: 340rpx;
}
.list_item{
  padding: 10rpx 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;
  font-size: 24rpx;
  color: #303133;  
  overflow: hidden;
}
.list_left{
  flex-basis: 80%;
}
.list_right{
  flex-basis: 20%;
  text-align: right;
}
.wx_btn{
  margin: 0;
  background: #1296db;
}
.list_sub_item{
  padding: 10rpx 20rpx;
  background: #f1f1f1;
}
.list_sub_item text{
  display: block;
  font-size: 22rpx;
  line-height: 30rpx;
}
.disnone{
  display: none;
}